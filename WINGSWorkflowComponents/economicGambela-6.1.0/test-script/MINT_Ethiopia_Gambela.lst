 117  abort$(sum(i, flag1(i)) gt 0) "cc1 not satisfied";
 118   
 119  psi(i) = sigma(i)*cyclesdata(i,'ybarN')/(supplyelasticity(i,'eta')*(1
 120           - cyclesdata(i,'ybarN')));
 121  cc2(i) = b(i)*supplyelasticity(i,'eta')*(1 - psi(i)) - sum(j$(ord(j) ne ord(i)),
 122           b(j)*supplyelasticity(j,'eta')*sqr(1 + (1/supplyelasticity(j,'eta')))*(1 + psi(j)
 123           - cyclesdata(j,'ybarN')));
 124  flag2(i) = 1$(cc2(i) gt 0);
 125  abort$(sum(i,flag2(i)) gt 0) "cc2 not satisfied";
 126  display cc1, cc2;
