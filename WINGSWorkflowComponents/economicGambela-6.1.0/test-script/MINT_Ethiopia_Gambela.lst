 128  scalars
 129  term             indicator equal to card(i) when all deltas converge /0/
 130  toler            tolerance for convergence /0.001/;
 131   
 132  parameters
 133  delta0(i)        myopic CES production function parameter
 134  adj(i)           adjustment term using myopic delta
 135  error(i)         absolute value of change in delta
 136  converge(i)      indicator equal to one when delta converges;
 137  delta0(i) = supplyelasticity(i,'eta')/(1 + supplyelasticity(i,'eta'));
 138  adj(i) = 1 - (b(i)/(delta0(i)*(1-delta0(i))))/(sum(j,
 139           (b(j)/(delta0(j)*(1-delta0(j)))) +
 140           (sigma(j)*b(j)*cyclesdata(j,'ybarN')/(delta0(j)*(delta0(j)
 141           - cyclesdata(j,'ybarN'))))));
 142   
 143  variables
 144  delta(i)         production function homogeneity parameters
 145  beta(i,l)        production function share parameters
 146  dummy            dummy objective;
 147  positive variables delta,beta;
 148   
 149  equations
 150  etacal(i)        calibration to exogenous supply elasticity
 151  nresp(i)         calibration against agronomic yield response to N
 152  betas(i)         summation constraint for share parameters
 153  edummy           dummy objective function;
 154  etacal(i).. supplyelasticity(i,'eta') =e= (delta(i)/(1-delta(i)))*adj(i);
 155  Nresp(i).. cyclesdata(i,'ybarN')*((beta(i,'land')*(calibdata(i,'xbar1')**rho(i))) +
 156            (beta(i,'fertilizer')*(calibdata(i,'xbar2')**rho(i)))) =e=
 157            delta.l(i)*beta(i,'fertilizer')*(calibdata(i,'xbar2')**rho(i));
 158  betas(i).. sum(l, beta(i,l)) =e= 1;
 159  edummy.. dummy =e= 0;
