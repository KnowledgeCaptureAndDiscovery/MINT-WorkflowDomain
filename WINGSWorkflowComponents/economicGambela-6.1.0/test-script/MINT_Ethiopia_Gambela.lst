 182  parameters
 183  mu(i)            scale parameters
 184  lbar1            initial shadow value of land
 185  lambda(i,l)      calibrated factor shadow values
 186  soccost(i,l)     social cost of inputs;
 187   
 188  mu(i) =  qbar(i)/((beta.l(i,'land')*(calibdata(i,'xbar1')**rho(i))) +
 189            (beta.l(i,'fertilizer')*calibdata(i,'xbar2')**rho(i)))**(delta.l(i)/rho(i));
 190  lbar1 = sum(i, (price(i,'p')*qbar(i)*(delta.l(i) - cyclesdata(i,'ybarN'))
 191          - productioncost(i,'c1')*calibdata(i,'xbar1'))*calibdata(i,'xbar1'))/sum(i, sqr(calibdata(i,'xbar1')));
 192  lambda(i,'land') = price(i,'p')*qbar(i)*(delta.l(i) - cyclesdata(i,'ybarN'))/calibdata(i,'xbar1')
 193               - (productioncost(i,'c1') + lbar1);
 194  lambda(i,'fertilizer') = price(i,'p')*qbar(i)*cyclesdata(i,'ybarN')/calibdata(i,'xbar2')
 195               - productioncost(i,'c2');
 196   
 197  display rho,delta.l,beta.l,mu,lbar1,lambda;
